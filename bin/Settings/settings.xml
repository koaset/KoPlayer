<?xml version="1.0"?>
<Settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <FormWidth>1058</FormWidth>
  <FormHeight>600</FormHeight>
  <StartupPlaylist>Shuffle Queue</StartupPlaylist>
  <Shufflequeue_SourcePlaylistName>Library</Shufflequeue_SourcePlaylistName>
  <Shufflequeue_NumPrevious>5</Shufflequeue_NumPrevious>
  <Shufflequeue_NumNext>15</Shufflequeue_NumNext>
  <RatingHotkeys>
    <Keys>OemPipe</Keys>
    <Keys>D1</Keys>
    <Keys>D2</Keys>
    <Keys>D3</Keys>
    <Keys>D4</Keys>
    <Keys>D5</Keys>
  </RatingHotkeys>
  <GlobalHotkeys>
    <GlobalHotkey>
      <Action>IncreaseVolume</Action>
      <Key>Insert</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
    <GlobalHotkey>
      <Action>DecreaseVolume</Action>
      <Key>Delete</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
    <GlobalHotkey>
      <Action>PlayOrPause</Action>
      <Key>Home</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
    <GlobalHotkey>
      <Action>ShowSongInfoPopup</Action>
      <Key>End</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
    <GlobalHotkey>
      <Action>PlayPreviousSong</Action>
      <Key>Prior</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
    <GlobalHotkey>
      <Action>PlayNextSong</Action>
      <Key>Next</Key>
      <Modifier>Control</Modifier>
    </GlobalHotkey>
  </GlobalHotkeys>
  <RunAtStartup>false</RunAtStartup>
  <MinimizeToTray>true</MinimizeToTray>
  <PopupOnSongChange>false</PopupOnSongChange>
  <ScrobblingEnabled>false</ScrobblingEnabled>
  <RowHeight>17</RowHeight>
  <FontName>Microsoft Sans Serif</FontName>
  <FontSize>8</FontSize>
</Settings>